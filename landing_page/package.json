        "eslint-webpack-plugin": "^3.2.0",
        "file-loader": "^6.2.0",
        "glob": "^8.0.3",
        "html-webpack-plugin": "^5.5.0",
        "mini-css-extract-plugin": "^2.6.1",
        "npm-run-all": "^4.1.5",
