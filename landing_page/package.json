{
    "name": "landing_page",
    "version": "1.0.0",
    "description": "Front end development setup for landing page website with Webpack",
    "main": "index.js",
    "scripts": {
        "dev": "webpack serve --config webpack.dev.config.js --hot",
        "sass-migrator": "sass-migrator module --verbose --migrate-deps src/sass/main.scss",
