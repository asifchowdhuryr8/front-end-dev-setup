{
  "name": "multiple_pages",
  "version": "1.0.0",
  "description": "Front end development setup for multiple pages website with Webpack",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --config webpack.dev.config.js --hot",
    "sass-migrator": "sass-migrator module --verbose --migrate-deps src/sass/main.scss",
    "build": "webpack --config webpack.production.config.js",
    "all": "npm-run-all --parallel sass-migrator build"
  },
  "keywords": [],
  "author": "ITS MAC",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "autoprefixer": "^10.4.14",
    "css-loader": "^6.7.3",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "glob": "^10.2.2",
    "html-webpack-plugin": "^5.5.1",
    "mini-css-extract-plugin": "^2.7.5",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.3.0",
    "purgecss-webpack-plugin": "^5.0.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "sass-migrator": "^1.7.3",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
